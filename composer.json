{
    "name": "baijunyao/laravel-bjyblog",
    "description": "Laravel blog.",
    "keywords": ["laravel", "blog", "php"],
    "authors": [
        {
            "name": "baijunyao",
            "email": "baijunyao@baijunyao.com",
            "homepage": "https://baijunyao.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "algolia/algoliasearch-client-php": "^2.5",
        "appstract/laravel-opcache": "^3.0",
        "baidu/aip-sdk": "dev-master#84e5936eb6426b4cc9d9a96848568b9dd527b4a0",
        "baijunyao/laravel-flash": "^2.0",
        "baijunyao/laravel-plugin-manager": "^3.0",
        "baijunyao/laravel-restful": "^3.0",
        "baijunyao/laravel-scout-elasticsearch": "^5.0",
        "baijunyao/laravel-support": "^1.4",
        "composer/semver": "^1.4",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "fukuball/jieba-php": "~0.25",
        "genealabs/laravel-model-caching": "^0.11",
        "graham-campbell/markdown": "^13.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "iidestiny/laravel-filesystem-oss": "^1.2",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "kalnoy/nestedset": "^5.0",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravelium/feed": "^8.0",
        "lcobucci/jwt": "3.3.3",
        "league/html-to-markdown": "^4.6",
        "mavinoo/laravel-batch": "^2.2",
        "mews/purifier": "^3.0",
        "overtrue/laravel-follow": "^1.1",
        "overtrue/laravel-lang": "^4.0",
        "paquettg/php-html-parser": "^2.1",
        "predis/predis": "^1.1",
        "ready2order/laravel-fluent-logger": "8.2",
        "sentry/sentry-laravel": "1.9.0",
        "socialiteproviders/qq": "^3.0",
        "socialiteproviders/vkontakte": "^4.0",
        "socialiteproviders/weibo": "^3.0",
        "spatie/laravel-backup": "^6.3",
        "stichoza/google-translate-php": "^4.0",
        "symfony/filesystem": "^5.1"
    },
    "require-dev": {
        "baijunyao/laravel-test-support": "^4.2",
        "barryvdh/laravel-ide-helper": "^2.4",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^2.3.6",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "^1.9.1",
        "itsgoingd/clockwork": "^4.0",
        "laravel/dusk": "^6.7",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "0.6.4",
        "phpstan/phpstan": "^0.12.34",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "itsgoingd/clockwork"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "exclude-from-classmap": [
            "tests/Commands/Upgrade/databases"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "phpcs": "vendor/bin/php-cs-fixer fix",
        "phpstan": "vendor/bin/phpstan analyse --ansi",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
